menuconfig EXAMPLES_TESTCASE_DM_ITC
  bool "Device Management ITC TestCase Example"
  default n
  depends on DM
  ---help---
    Enable the Device Management ITC TestCase Example

if EXAMPLES_TESTCASE_DM_ITC

menuconfig EXAMPLES_TESTCASE_DM_WIFI
  bool "Set Up Wifi Info For DM Demo"
  default n
  ---help---
    Enable the artik demo example

if EXAMPLES_TESTCASE_DM_WIFI

config DM_AP_SSID
  string "wifi ap ssid"
  default "SSID"

config DM_AP_PASS
  string "wifi ap password"
  default "PASSWORD"

config DM_AP_SECURITY
  string "wifi join security"
  default "wpa2_aes"

endif #EXAMPLES_TESTCASE_DM_WIFI


